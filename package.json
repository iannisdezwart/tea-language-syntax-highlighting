{
    "name": "tea-language-syntax-highlighting",
    "displayName": "Tea Language Syntax Highlighting",
    "description": "Highlights Your Tea Files",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.56.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "tea",
            "aliases": ["Tea", "tea"],
            "extensions": [".tea .t"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "tea",
            "scopeName": "source.tea",
            "path": "./syntaxes/tea.tmLanguage.json"
        }]
    }
}